{
  "name": "berlin-lea-termin-bot",
  "version": "0.0.0",
  "author": "David Leclerc",
  "main": "./src/index.ts",
  "scripts": {
    "start": "cross-env ENV=development nodemon ./src/index.ts --poll",
    "parse": "cross-env ENV=development nodemon ./src/index.ts --parse",
    "collect": "npm run clean && npm run build && cross-env ENV=production node ./dist/src/index.js --poll --endless",
    "build": "tsc -p .",
    "clean": "rimraf ./dist"
  },
  "dependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.11.18",
    "@types/selenium-webdriver": "^4.1.16",
    "chart.js": "^3.9.1",
    "chartjs-node-canvas": "^4.1.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "minimist": "^1.2.8",
    "nodemon": "^3.0.1",
    "pino": "^8.15.0",
    "pino-pretty": "^10.2.0",
    "selenium-webdriver": "^4.12.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
